<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head.ejs') %>
</head>

<body>
    <header>
        <%- include('./partials/header.ejs') %>
        <nav>
            <%- include('./partials/nav.ejs') %>
        </nav>
    </header>

    <div class="body-home">
        <div class="weather">
            <p>Singapore</p>
            <p><span class="today"></span></p>
            <p>Temperature: <span class="tempt"></span>&#176;C</p>
            <p>Temp-min: <span class="min-tempt"></span> &#176;C </p>
            <p>Temp-max: <span class="max-tempt"></span>&#176;C</p>
            <p>Humidity: <span class="humidity"></span>%</p>
            <p>Description: <span class="describe"></span></p>
            <p>Main: <span class="main"></span></p>
        </div>
        <section class="list-users">
            <% users.forEach(user => { %>
            <div class="each-user">
                <p><img src="<%= currentUser.avata; %>" class="avata-user"><a href="/lico/<%= currentUser.userName; %>">
                        <%= user.userName; %></a></p>
                <div><a href="/lico/<%= currentUser.userName; %>" class="btn-page">Explore</a></div>
            </div>
            <% }) %>
        </section>
    </div>

</body>

</html>