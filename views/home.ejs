<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head.ejs') %>
</head>

<body>
    <header>
        <%- include('./partials/header.ejs') %>
        <nav>
            <%- include('./partials/nav.ejs') %>
        </nav>
    </header>

    <div class="body-home">
        <div class="weather card shadow-lg p-3 mb-5 rounded">
            <p class="slogan">Singapore</p>
            <p class="date"><span class="today"></span></p>
           
            <p>	&#127777; <span class="tempt"></span>&#176;C </p>
            <p class="date">(<span class="min-tempt"></span> &#176;C - <span class="max-tempt"></span>&#176;C)</p>
            <p>	&#128167; <span class="humidity"></span>%</p>
            <p>&#9729; <span class="clouds"></span>%</p>
          
            <p><span class="describe"></span></p>
            
        </div>
        <section class="list-users">
            <% users.forEach(user => { %>
            <div class="each-user">
                <p><img src="<%= user.avata; %>" class="avata-user"><a href="/lico/<%= currentUser.userName; %>">
                        <%= user.userName; %></a></p>
                <div><a href="/lico/<%= currentUser.userName; %>" class="btn-page">Explore</a></div>
            </div>
            <% }) %>
        </section>
    </div>

</body>

</html>